generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Task {
  id    Int     @id @default(autoincrement())
  label String
  done  Boolean @default(false)
}

model Clothes {
  id       Int    @id @default(autoincrement())
  name     String
  category String
  color    String
  size     String
}
